def matrixRe(self,nums,r,c):
  flat=sum(nums,[])
  if len(flat)!=r*c:
    return nums
  tuples=zip(*（[iter(flat)]*c）
  return map(list,tuples)


import numpy as np
class Solution(object):
  def matrixReshape(self,nums,r,c):
    try:
      return np.reshape(nums,(r,c)).tolist()
     wxcept:
      return nums
